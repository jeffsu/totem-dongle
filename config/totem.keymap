            label =                                                                                "COLEMAK";
            bindings = <
            //               ┏┳┳━━━━━━━━━━━┳┳┳━━━━━━━━━━━┳┳┳━━━━━━━━━━━┳┳┳━━━━━━━━━━━┳┳┳━━━━━━━━━━━┳┳┓  ┏┳┳━━━━━━━━━━━┳┳┳━━━━━━━━━━━┳┳┳━━━━━━━━━━━┳┳┳━━━━━━━━━━━┳┳┳━━━━━━━━━━━┳┳┓
